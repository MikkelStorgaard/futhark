runtime_file = None
do_warmup_run = False
num_runs = 1
entry_point = "main"
parser = argparse.ArgumentParser(description="A compiled Futhark program.")
parser.add_argument("-t", "--write-runtime-to", action="append", default=[])
parser.add_argument("-r", "--runs", action="append", default=[])
parser.add_argument("-e", "--entry-point", action="append", default=[])
parser.add_argument("-b", "--binary-output", action="append_const", default=[],
                    const=None)
parser_result = vars(parser.parse_args(sys.argv[1:]))
for optarg in parser_result["write_runtime_to"]:
  if runtime_file:
    runtime_file.close()
  runtime_file = open(optarg, "w")
for optarg in parser_result["runs"]:
  num_runs = optarg
  do_warmup_run = True
for optarg in parser_result["entry_point"]:
  entry_point = optarg
for optarg in parser_result["binary_output"]:
  pass
class internal:
  def __init__(self):
    pass
  def futhark_main(self, a_mem_sizze_2497, a_mem_2498, sizze_2484):
    binop_x_2500 = sext_i32_i64(sizze_2484)
    bytes_2499 = (binop_x_2500 * np.int64(4))
    mem_2501 = allocateMem(bytes_2499)
    acc_2491 = np.int32(0)
    i_2490 = np.int32(0)
    one_2512 = np.int32(1)
    for counter_2511 in range(sizze_2484):
      binop_param_y_2488 = indexArray(a_mem_2498, (i_2490 * np.int32(4)),
                                      ct.c_int32, np.int32)
      res_2489 = (acc_2491 + binop_param_y_2488)
      writeScalarArray(mem_2501, (i_2490 * np.int32(4)), ct.c_int32(res_2489))
      row_2495 = indexArray(mem_2501, (i_2490 * np.int32(4)), ct.c_int32,
                            np.int32)
      acc_tmp_2509 = row_2495
      acc_2491 = acc_tmp_2509
      i_2490 += one_2512
    discard_2496 = acc_2491
    out_arrsizze_2508 = sizze_2484
    out_memsizze_2507 = bytes_2499
    out_mem_2506 = mem_2501
    return (out_memsizze_2507, out_mem_2506, out_arrsizze_2508)
self = internal()
def entry_main():
  a_mem_2498_ext = read_array(input_stream, FUTHARK_INT32, 1, np.int32)
  if not(((type(a_mem_2498_ext) in [np.ndarray]) and (a_mem_2498_ext.dtype == np.int32))):
    raise TypeError("Argument #0 has invalid value\nFuthark type: {}\nType of given Python value: {}\n".format("[]i32",
                                                                                                               type(a_mem_2498_ext)))
  try:
    assert (sizze_2484 == np.int32(a_mem_2498_ext.shape[0])), 'variant dimension wrong'
  except NameError as e:
    sizze_2484 = np.int32(a_mem_2498_ext.shape[0])
  a_mem_sizze_2497 = np.int32(a_mem_2498_ext.nbytes)
  a_mem_2498 = unwrapArray(a_mem_2498_ext)
  try:
    with np.errstate(divide="ignore", over="ignore", under="ignore",
                     invalid="ignore"):
      if do_warmup_run:
        a_mem_copy_2513 = allocateMem(a_mem_2498_ext.nbytes)
        ct.memmove(addressOffset(a_mem_copy_2513, 0, ct.c_byte),
                   addressOffset(a_mem_2498, 0, ct.c_byte),
                   a_mem_2498_ext.nbytes)
        (out_memsizze_2507, out_mem_2506,
         out_arrsizze_2508) = self.futhark_main(a_mem_sizze_2497,
                                                a_mem_copy_2513, sizze_2484)
      for i in range(int(num_runs)):
        a_mem_copy_2513 = allocateMem(a_mem_2498_ext.nbytes)
        ct.memmove(addressOffset(a_mem_copy_2513, 0, ct.c_byte),
                   addressOffset(a_mem_2498, 0, ct.c_byte),
                   a_mem_2498_ext.nbytes)
        time_start = time.time()
        (out_memsizze_2507, out_mem_2506,
         out_arrsizze_2508) = self.futhark_main(a_mem_sizze_2497,
                                                a_mem_copy_2513, sizze_2484)
        time_end = time.time()
        if runtime_file:
          runtime_file.write(str((int((time_end * 1000000)) - int((time_start * 1000000)))))
          runtime_file.write("\n")
  except AssertionError as e:
    sys.exit("Assertion.{} failed".format(e))
  if runtime_file:
    runtime_file.close()
  if (np.product([out_arrsizze_2508]) == 0):
    sys.stdout.write("empty(i32)")
  else:
    print_first_2515 = True
    sys.stdout.write("[")
    for print_elem_2514 in createArray(ct.cast(out_mem_2506, ct.POINTER(ct.c_int32)),
                                       (out_arrsizze_2508,)):
      if not(print_first_2515):
        sys.stdout.write(", ")
      sys.stdout.write(("%di32" % print_elem_2514))
      print_first_2515 = False
    sys.stdout.write("]")
  sys.stdout.write("\n")
entry_points = {"main": entry_main}
entry_point_fun = entry_points.get(entry_point)
if (entry_point_fun == None):
  sys.exit("No entry point '{}'.  Select another with --entry point.  Options are:\n{}".format(entry_point,
                                                                                               "\n".join(entry_points.keys())))
else:
  entry_point_fun()